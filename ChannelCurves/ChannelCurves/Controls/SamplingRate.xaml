<UserControl x:Class="ChannelCurves.Controls.SamplingRate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChannelCurves.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <GroupBox Header="采样率">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <RadioButton Content="1 M" Grid.Row="0"/>
            <RadioButton Content="5 M" Grid.Row="1"/>
            <RadioButton Content="10 M" Grid.Row="2"/>
            <RadioButton Content="20 M" Grid.Row="3"/>
            <RadioButton Content="40 M" Grid.Row="4"/>
        </Grid>
    </GroupBox>
</UserControl>
