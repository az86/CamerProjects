<UserControl x:Class="ChannelCurves.View.ReferenceVoltage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChannelCurves.View"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <GroupBox Header="参考电压">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <RadioButton Content="2.5 V" Grid.Row="0"/>
            <RadioButton Content="3.3 V" Grid.Row="1"/>
            <RadioButton Content="4.096 V" Grid.Row="2"/>
            <RadioButton Content="5 V" Grid.Row="3"/>
        </Grid>
    </GroupBox>
</UserControl>
