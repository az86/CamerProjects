<UserControl x:Class="ChannelCurves.View.ChannelBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChannelCurves.View"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <GroupBox Header="通道数">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <!--<RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>-->
            </Grid.RowDefinitions>
            <RadioButton Content="1" Grid.Row="0"/>
            <RadioButton Content="2" Grid.Row="1"/>
            <!--<RadioButton Content="4" Grid.Row="2"/>
            <RadioButton Content="8" Grid.Row="3"/>
            <RadioButton Content="16" Grid.Row="4"/>-->
        </Grid>
    </GroupBox>
</UserControl>
